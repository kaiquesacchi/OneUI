{
  "printWidth": 100,
  "tabWidth": 2,
  "tabs": false,
  "semi": true,
  "singleQuote": false,
  "quoteProps": "as-needed",
  "jsxSingleQuote": false,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowFunctionParentheses": "always",
  "requirePragma": false,
  "insertPragma": false,
  "endOfLine": "lf",
  "singleAttributePerLine": false
}
